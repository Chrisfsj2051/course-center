<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >

    <context id="context1" >
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!-- jdbc连接 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://139.224.137.85:3306/temp"
                        userId="root" password="" />

        <!-- 生成模型的包名和位置 -->
        <javaModelGenerator targetPackage="com.cxgzs.seweb.model" targetProject="src/main/java" />

        <!-- 生成的映射文件包名和位置 -->
        <sqlMapGenerator targetPackage="com.cxgzs.seweb.mapping" targetProject="src/main/java" />

        <!-- 生成DAO的包名和位置 -->
        <javaClientGenerator targetPackage="com.cxgzs.seweb.dao" targetProject="src/main/java" type="XMLMAPPER" />

        <table tableName="%"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false" >
        </table>
    </context>
</generatorConfiguration>
